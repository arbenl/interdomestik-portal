    "eslint-plugin-react-refresh": "^0.4.20",
    "jsdom": "^26.1.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7",
    "vite-node": "^3.0.0",
