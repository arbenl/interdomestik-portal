{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host localhost",
    "dev:e2e": "vite --host localhost --mode e2e",
    "typecheck": "tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.vitest.json --noEmit && tsc -p tsconfig.tools.json --noEmit",
    "lint": "eslint .",
    "build": "vite build",
    "test": "vitest",
    "test:ci": "vitest --run --coverage",
    "e2e": "playwright test",
    "verify:frontend": "pnpm typecheck && pnpm lint && pnpm build && pnpm test:ci # && pnpm e2e"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^8.0.0",
    "@radix-ui/react-dialog": "workspace:*",
    "@tanstack/react-query": "^5.40.0",
    "firebase": "^12.2.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.51.5",
    "react-router-dom": "^7.8.2",
    "sonner": "^2.0.7",
    "zod": "^4.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.45.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "eslint": "^9.36.0",
    "eslint-config-custom": "workspace:*",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "undici": "^7.16.0",
    "vite": "^7.1.7",
    "vite-node": "^3.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.0"
  }
}
