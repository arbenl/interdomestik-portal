{
  "name": "interdomestik-member-portal",
  "version": "1.0.0",
  "description": "Member portal for Interdomestik organization",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm --filter frontend dev",
    "dev:emu": "firebase emulators:start --project interdomestik-dev",
    "seed:raw": "tsx --tsconfig scripts/tsconfig.scripts.json scripts/emulator/seed.ts",
    "dev:seed": "tsx --tsconfig scripts/tsconfig.scripts.json scripts/emulator/run-seed.ts",
    "emu:seed": "firebase emulators:exec --project interdomestik-dev \"pnpm seed:raw\"",
    "dev:emu:seed": "concurrently \"pnpm dev:emu\" \"pnpm dev:seed\"",
    "dev:stop": "kill-port 9099 8080 5001 5000 4400 4500",
    "build": "pnpm --filter frontend build",
    "lint": "pnpm --filter frontend lint && pnpm --filter functions lint",
    "format": "prettier --write .",
    "knip": "knip",
    "test": "pnpm --filter frontend test && pnpm --filter functions test",
    "typecheck": "pnpm --filter frontend typecheck && pnpm --filter functions typecheck",
    "test:ci": "pnpm --filter frontend test:run && pnpm --filter functions test",
    "test:rules": "vitest -c vitest.config.rules.ts",
    "check": "node scripts/check-errors.js",
    "verify:frontend": "pnpm --filter frontend typecheck && pnpm --filter frontend lint --no-fix && pnpm --filter frontend build && pnpm --filter frontend test:ci",
    "specify": "uvx --from git+https://github.com/github/spec-kit.git specify",
    "mcp": "pnpm --filter mcp-servers start",
    "orchestrate:e2e": "tsx scripts/orchestrate-e2e.ts"
  },
  "keywords": [
    "member-portal",
    "firebase",
    "interdomestik"
  ],
  "author": "Manus AI",
  "license": "MIT",
  "dependencies": {
    "firebase": "^12.2.1",
    "firebase-admin": "^13.5.0"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^5.0.0",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.3.3",
    "@vitest/coverage-v8": "^1.6.1",
    "chai": "^6.0.1",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.2",
    "eslint-config-prettier": "^10.1.8",
    "firebase-tools": "^14.16.0",
    "husky": "^9.1.7",
    "kill-port": "^2.0.1",
    "knip": "^5.64.1",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "mocha": "^11.7.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "wait-on": "^9.0.1"
  },
  "engines": {
    "node": ">=20.19.4 <21",
    "pnpm": ">=10.16.1 <11"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,json,css,md}": "prettier --write",
    "frontend/src/**/*.{ts,tsx}": [
      "npm --prefix frontend run lint",
      "sh -c 'npm --prefix frontend run typecheck'"
    ],
    "functions/src/**/*.ts": [
      "npm --prefix functions run lint",
      "sh -c 'npm --prefix functions run typecheck'"
    ]
  },
  "packageManager": "pnpm@10.16.1+sha521.0e155aa2629db8672b49e8475da6226aa4bdea85fdcdfdc15350874946d4f3c91faaf64cbdc4a5d1ab8002f473d5c3fcedcd197989cf0390f9badd3c04678706"
}
