#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/arbenlila/development/interdomestik/interdomestik-member-portal/node_modules/.pnpm/firebase-tools@14.16.0_@types+node@24.3.3_encoding@0.1.13/node_modules/firebase-tools/lib/bin/node_modules:/Users/arbenlila/development/interdomestik/interdomestik-member-portal/node_modules/.pnpm/firebase-tools@14.16.0_@types+node@24.3.3_encoding@0.1.13/node_modules/firebase-tools/lib/node_modules:/Users/arbenlila/development/interdomestik/interdomestik-member-portal/node_modules/.pnpm/firebase-tools@14.16.0_@types+node@24.3.3_encoding@0.1.13/node_modules/firebase-tools/node_modules:/Users/arbenlila/development/interdomestik/interdomestik-member-portal/node_modules/.pnpm/firebase-tools@14.16.0_@types+node@24.3.3_encoding@0.1.13/node_modules:/Users/arbenlila/development/interdomestik/interdomestik-member-portal/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/arbenlila/development/interdomestik/interdomestik-member-portal/node_modules/.pnpm/firebase-tools@14.16.0_@types+node@24.3.3_encoding@0.1.13/node_modules/firebase-tools/lib/bin/node_modules:/Users/arbenlila/development/interdomestik/interdomestik-member-portal/node_modules/.pnpm/firebase-tools@14.16.0_@types+node@24.3.3_encoding@0.1.13/node_modules/firebase-tools/lib/node_modules:/Users/arbenlila/development/interdomestik/interdomestik-member-portal/node_modules/.pnpm/firebase-tools@14.16.0_@types+node@24.3.3_encoding@0.1.13/node_modules/firebase-tools/node_modules:/Users/arbenlila/development/interdomestik/interdomestik-member-portal/node_modules/.pnpm/firebase-tools@14.16.0_@types+node@24.3.3_encoding@0.1.13/node_modules:/Users/arbenlila/development/interdomestik/interdomestik-member-portal/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/firebase-tools@14.16.0_@types+node@24.3.3_encoding@0.1.13/node_modules/firebase-tools/lib/bin/firebase.js" "$@"
else
  exec node  "$basedir/../.pnpm/firebase-tools@14.16.0_@types+node@24.3.3_encoding@0.1.13/node_modules/firebase-tools/lib/bin/firebase.js" "$@"
fi
